
project Apq_postgresqlhelp_bd is

   version := $version;

   for Languages use ("C");
   for Source_Dirs use ("src-c");
   for Object_Dir use "obj-c";
   for Library_Name use "apq-postgresqlhelp" ;
   for Library_Dir use "lib-c";
   for Library_Kind use "dynamic";
   for Library_Version use "libapq-postgresqlhelp.so." & version ;
   for Library_Options use ("-Wl,--export-dynamic", $LIBDIR2 , "-lssl" , "-lpq");
   for Externally_Built use "false";

package Compiler is
      for Default_Switches ("c") use ("-O2");
   end Compiler;

end Apq_postgresqlhelp_bd;

