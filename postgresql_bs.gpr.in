-- more or less dummy file without "--with apq" for gprclean :-)
-- don't clean "apq"

project postgresql_bs is

   version := $version;

   for Languages use ( "ADA" , "C");
   for Source_Dirs use ("src");
   for Object_Dir use "obj/static";
   for Library_ALI_Dir use "lib_ali_static";
   for Library_Name use "apq-postgresql" ;
   for Library_Dir use "lib-static";
   for Library_Kind use "static";
   for Library_Options use ( $LIBDIR & "/../lib-c-static", "-lssl" , "-lpq", "-lapq-postgresqlhelp" );
   for Externally_Built use "false";

package Compiler is
      for Default_Switches ("c") use ("-O2","-gnat05");
   end Compiler;

end postgresql_bs;

